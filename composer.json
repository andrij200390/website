{
    "name": "outstyle/website",
    "description": "Outstyle social network, based on Yii2 AT",
    "keywords": ["yii2", "framework", "advanced", "application template", "social network", "hip-hop"],
    "homepage": "https://outstyle.org",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/outstyle"
    },
    "minimum-stability": "dev",
    "repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		},
        {
            "type": "vcs",
            "url": "https://github.com/scsmash3r/yii2-image-uploader"
        },
        {
            "type": "vcs",
            "url": "https://github.com/scsmash3r/yii2-cropper"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
		
        "yurkinx/yii2-image": "dev-master",
        "demi/image": "dev-master",
        "demi/cropper": "dev-master",
        "developeruz/yii2-db-rbac": "dev-master",
        "vova07/yii2-imperavi-widget": "dev-master",
        "dragonjet/yii2-opengraph": "dev-master",
        "fg/multiplayer": "dev-master",
        "mihaildev/yii2-elfinder": "dev-master",
        "sadovojav/yii2-ckeditor": "dev-master",
        "himiklab/yii2-sitemap-module": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
		"aalaap/faker-youtube": "dev-master",
		
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    }
}
